class ListResponse<T> {
  ListResponse({
    required this.count,
    required this.pageSize,
    required this.page,
    required this.data,
  });

  ListResponse.empty() : count = 0, pageSize = 0, page = 0, data = [];

  final int count;
  final int pageSize;
  final int page;
  final List<T> data;
}
