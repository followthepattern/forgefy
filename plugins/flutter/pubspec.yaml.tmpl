name: adapticc
description: "adapticc mobile app"
publish_to: 'none'
version: 0.1.0

environment:
  sdk: ^3.8.1

dependencies:
  flutter:
    sdk: flutter
  flutter_form_builder: ^10.0.1
  flutter_localizations:
    sdk: flutter
  flutter_secure_storage: ^9.2.4
  flutter_svg: ^2.1.0
  fluttertoast: ^8.2.12
  form_builder_image_picker: ^4.3.1
  form_builder_validators: ^11.1.2
  go_router: ^15.2.0
  graphql: ^5.2.1
  http: ^1.4.0
  intl: ^0.20.2
  provider: ^6.1.5
  skeletonizer: ^2.0.1
  two_dimensional_scrollables: ^0.3.6

dev_dependencies:
  flutter_test:
    sdk: flutter
  flutter_lints: ^5.0.0
  flutter_launcher_icons: "^0.14.4"

flutter:
  uses-material-design: true
  generate: true

flutter_launcher_icons:
  android: "launcher_icon"
  ios: true
  image_path: ""
  min_sdk_android: 21 # android min sdk min:16, default 21
