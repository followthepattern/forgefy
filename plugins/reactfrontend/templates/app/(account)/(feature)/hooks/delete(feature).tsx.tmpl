import { delete{{ .FeatureTypeName }} as graphQL } from "../graphql/mutation";
import { DeleteMutationResult, MutationResourceResponse } from "@/graphql/mutation";
import useDelete from "@/graphql/hooks/useDelete";

export default function useDelete{{ .FeatureTypeName }}(): DeleteMutationResult<string,number | undefined> {
    const parseResult = (data?: MutationResponse | null): number | undefined => {
        return data?.{{ .FeatureNameLowerCamel }}s?.delete?.code
    }

    return useDelete({parseResult, graphQL})
}

export interface MutationResponse {
    {{ .FeatureNameLowerCamel }}s: MutationResourceResponse
}